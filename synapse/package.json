    "config-browserify": "^1.0.5",
    "connect-history-api-fallback": "^1.3.0",
    "d3": "^4.1.1",
    "es6-promise": "^3.2.1",
    "isomorphic-fetch": "^2.2.1",
    "moment": "^2.13.0",
    "react": "^15.1.0",
    "react-bootstrap": "^0.29.5",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.7.0",
    "react-tooltip": "^3.3.0",
    "redux": "^3.5.2",
    "redux-saga": "^0.11.1",
    "regression": "^1.2.1",
    "simple-statistics": "^2.2.0",
    "sum-values-in-object": "^1.0.5",
    "underscore": "^1.8.3",
    "whippersnapper": "<1.0.0",
    "xhr": "^2.2.2",
    "zzzss": "0.1.1"
  }
}
