{
  "name": "synapse",
  "version": "1.1.1",
  "description": "Management information dashboard.",
  "main": "dev-server.js",
  "scripts": {
    "build": "echo 'Checking for outdated dependencies...' && (npm outdated || true) && npm run bundle",
    "bundle": "./node_modules/webpack/bin/webpack.js --config webpack.config.js",
    "lint": "npm run lint:js && npm run lint:scss && npm run lint:html",
    "lint:js": "eslint 'src/js/**/*.js' 'src/js/**/*.jsx' 'test/**/*'",
    "lint:scss": "./node_modules/sass-lint/bin/sass-lint.js --verbose --no-exit",
    "lint:html": "./node_modules/htmlhint/bin/htmlhint 'src/**/*.html'",
    "start": "NODE_ENV=development ./node_modules/webpack-dashboard/bin/webpack-dashboard.js -c cyan -t Synapse -- node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --config webpack.config.js --inline",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "storybook:build": "build-storybook -c .storybook -o storybook",
    "test": "NODE_ENV=test nyc mocha --recursive --compilers js:babel-core/register --require babel-polyfill test/unit",
    "test:ci": "NODE_ENV=test mocha --recursive --compilers js:babel-core/register --require babel-polyfill --reporter mocha-jenkins-reporter -O junit_report_path=./reports/test-results.xml,junit_report_name=\"Tests Results\",junit_report_stack=1 test/unit",
    "test:acceptance": "NODE_ENV=test mocha --timeout 30000 --require co-mocha --require babel-register --require babel-polyfill --recursive test/acceptance/specs",
    "test:acceptance:ci": "NODE_ENV=test mocha --timeout 60000 --require co-mocha --require babel-register --require babel-polyfill --recursive --reporter mocha-jenkins-reporter -O junit_report_path=./reports/acceptance-test-results.xml,junit_report_name=\"Acceptance Tests\",junit_report_stack=1 test/acceptance/specs",
    "validate": "npm ls --production",
    "version": "git add package.json",
    "postversion": "git push && git push origin HEAD:master && git push --tags",
    "generate-test-data": "node testApi/scripts/generateTestData"
  },
  "pre-commit": [
    "lint",
    "test",
    "validate"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/digitalrigbitbucketteam/synapse.git"
  },
  "author": "Buildit",
  "license": "MIT",
  "homepage": "https://bitbucket.org/digitalrigbitbucketteam/synapse#readme",
  "devDependencies": {
    "@kadira/storybook": "^2.15.1",
    "@kadira/storybook-addon-notes": "^1.0.1",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-plugin-webpack-alias": "^2.1.1",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.14.0",
    "babel-root-slash-import": "^1.1.0",
    "bootstrap-sass": "^3.3.7",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chromedriver": "2.30.0",
    "clean-webpack-plugin": "^0.1.13",
    "co-mocha": "^1.1.3",
    "connect-history-api-fallback": "^1.3.0",
    "cowsay": "^1.1.8",
    "css-loader": "^0.25.0",
    "del": "^2.2.1",
    "enzyme": "^2.3.0",
    "eslint": "^3.5.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-import-resolver-webpack": "^0.6.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-import": "^1.9.2",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.22.0",
    "htmlhint": "^0.9.13",
    "htmlhint-loader": "^1.0.0",
    "jsdom": "^9.8.0",
    "jsdom-global": "^2.1.0",
    "json-loader": "^0.5.4",
    "mocha": "^3.0.2",
    "mocha-jenkins-reporter": "^0.2.6",
    "node-sass": "^3.10.1",
    "nyc": "^8.3.2",
    "path": "^0.12.7",
    "pre-commit": "^1.1.3",
    "randomstring": "^1.1.5",
    "react-addons-test-utils": "^15.1.0",
    "redux-logger": "^2.7.4",
    "sass-lint": "^1.9.1",
    "sass-loader": "^4.0.2",
    "sasslint-webpack-plugin": "^1.0.4",
    "selenium-webdriver": "^3.0.0-beta-2",
    "sinon": "^1.17.6",
    "style-loader": "^0.13.1",
