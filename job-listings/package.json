{
  "name": "@buildit/job-listings",
  "version": "1.0.2",
  "description": "Small JS library for fetching Buildit job listings data from SmartRecruiter's API",
  "main": "lib/index.js",
  "files": [
    "lib/",
    "src/*.js"
  ],
  "scripts": {
