<section{{#section.class}} class="{{section.class}}"{{/section.class}}>
  {{#section.title}}
    <h2>{{{section.title}}}</h2>
  {{/section.title}}
  {{#section.content}}
    {{#paragraph}}<p>{{{paragraph}}}</p>{{/paragraph}}
    {{#blockquote}}<blockquote>{{{blockquote}}}</blockquote>{{/blockquote}}
    {{#inlineRow}}{{>molecules-list-inline-row}}{{/inlineRow}}
    {{#imgCards}}{{>molecules-list-img-cards}}{{/imgCards}}
    {{#imgLinks}}{{>molecules-list-image-links}}{{/imgLinks}}
    {{#twoColumnsList}}
      <ul class="grav-c-two-columns-text">
        {{#items}}
        <li>{{.}}</li>
        {{/items}}
      </ul>
    {{/twoColumnsList}}
  {{/section.content}}
</section>